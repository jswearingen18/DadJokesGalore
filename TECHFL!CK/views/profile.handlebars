<div class="profile-name">
  <h1>Hello, {{name}}</h1>
</div>
<div class="cards">
{{#each jokes as |joke|}}
  <div class="card">
    <div class="card-container">
      <p>{{jokes}}</p>
    </div>
  </div>
{{/each}}
</div>


<div id='login'>
  <h1 class='loginh1'>Submit a Joke:</h1>

  <form class='login-page'>
    <div class='loginContainer'>
      <label for='pwrd'><b>Joke:</b></label>
      <input
      id='jokes'
        type='text'
        placeholder='Enter Joke..'
        name='jokes'
        required
      />

      <button class='loginBtn' id='submitJoke' type='submit'>Submit Joke</button>
    </div>
  </form>
</div>

<script src='/js/profile.js'></script>